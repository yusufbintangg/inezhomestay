{
  "name": "INEZ Homestay - Authentic Indonesian Hospitality",
  "short_name": "INEZ Homestay",
  "description": "Experience authentic Indonesian hospitality at INEZ Homestay. Comfortable rooms, local culture, and warm welcome in the heart of Yogyakarta.",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#ffffff",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en-US",
  "dir": "ltr",
  "categories": ["travel", "hospitality", "accommodation", "booking"],
  "screenshots": [
    {
      "src": "/herohome/ZNH02290 - ARIEF DARU.jpg",
      "sizes": "1280x720",
      "type": "image/jpeg",
      "form_factor": "wide",
      "label": "INEZ Homestay Hero View"
    },
    {
      "src": "/rooms/studio1Bedroom/740618453.jpg",
      "sizes": "750x1334",
      "type": "image/jpeg",
      "form_factor": "narrow",
      "label": "Studio Room Interior"
    }
  ],
  "icons": [
    {
      "src": "/logo/Logo INEZ Homestay - ARIEF DARU.png",
      "sizes": "16x16",
      "type": "image/x-icon",
      "purpose": "any maskable"
    },
    {
      "src": "/logo/Logo INEZ Homestay - ARIEF DARU.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/logo/Logo INEZ Homestay - ARIEF DARU.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/logo/Logo INEZ Homestay - ARIEF DARU.png",
      "sizes": "1024x1024",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "shortcuts": [
    {
      "name": "Book Now",
      "short_name": "Book",
      "description": "Quick booking for rooms",
      "url": "/contact",
      "icons": [
        {
          "src": "/logo/inezhomestaylogo.ico",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "View Rooms",
      "short_name": "Rooms",
      "description": "Browse available rooms",
      "url": "/rooms",
      "icons": [
        {
          "src": "/logo/inezhomestaylogo.ico",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Contact Us",
      "short_name": "Contact",
      "description": "Get in touch with us",
      "url": "/contact",
      "icons": [
        {
          "src": "/logo/inezhomestaylogo.ico",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://inezhomestay.com/manifest.json"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp"]
      }
    }
  ],
  "share_target": {
    "action": "/contact",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "photos",
          "accept": ["image/jpeg", "image/png", "image/webp", "image/gif"]
        }
      ]
    }
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+inez",
      "url": "/?protocol=%s"
    }
  ],
  "note_taking": {
    "new_note_url": "/contact?note=%s"
  },
  "iarc_rating_id": ""
}
